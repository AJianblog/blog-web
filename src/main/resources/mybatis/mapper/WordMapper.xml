<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hzj.myblog.mapper.WordMapper">
    <insert id="addWordTOA" parameterType="com.hzj.myblog.entity.Word">
        INSERT INTO a(word, translation, phonetic)
        VALUE (#{word}, #{translation}, #{phonetic})
    </insert>
</mapper>